FUNCTION "algoma.pm::ZTF_JEST_STAT_TRANS" ( ) 
	RETURNS TABLE
	(
		"OBJNR" VARCHAR,
		"STATUS" VARCHAR
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
		AS
BEGIN
/***************************** 
	Write your function logic
 *****************************/
 	RETURN
 	
 		SELECT OBJNR,STRING_AGG(TXT04,' ') AS STATUS
		FROM (SELECT DISTINCT OBJNR,TXT04 FROM "algoma.pm::ZHV_PM_JEST_NE_TECO_INACT" WHERE STAT LIKE 'I%')
		GROUP BY OBJNR;
END;