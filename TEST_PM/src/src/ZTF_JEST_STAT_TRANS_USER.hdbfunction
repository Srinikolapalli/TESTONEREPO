FUNCTION "algoma.pm::ZTF_JEST_STAT_TRANS_USER" ( ) 
	RETURNS TABLE
	(
		"OBJNR" VARCHAR,
		"STATUS" VARCHAR
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	
	AS
BEGIN
/***************************** 
	Write your function logic
 *****************************/
 	RETURN
 	
 		SELECT OBJNR,STRING_AGG(TXT04_USER,' ') AS STATUS
		FROM (SELECT DISTINCT OBJNR,TXT04_USER FROM "algoma.pm::ZHV_PM_JEST_NE_TECO_INACT" WHERE TXT04_USER IN ('RTSE','PLCP'))
		GROUP BY OBJNR;
END;